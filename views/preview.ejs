<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Privacy Scraper</title>
  </head>
  <body>
    <h1>Preview</h1>
    <div>
      <button id="confirm">Confirm</button>
    </div>
    <table id="items">
      <thead>
        <tr>
          <th>#</th>
          <th>Company</th>
          <th>Website</th>
        </tr>
      </thead>
      <tbody>
        <% for (const item of items) { %>
        <tr>
          <td><%= item.id %></td>
          <td><%= item.name %></td>
          <td><%= item.website %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
    <script src="preview.js"></script>
  </body>
</html>
